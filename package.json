{
    "name": "@airtop/sdk",
    "version": "0.1.43",
    "private": false,
    "repository": "https://github.com/airtop-ai/airtop-node-sdk",
    "main": "./index.js",
    "types": "./index.d.ts",
    "scripts": {
        "format": "prettier . --write --ignore-unknown",
        "build": "tsc",
        "prepack": "cp -rv dist/. .",
        "test": "jest",
        "prepublish": "yarn build",
        "biome:lint": "biome lint --diagnostic-level=error --no-errors-on-unmatched src",
        "biome:fix": "biome format --write && biome lint --write",
        "biome:format": "biome format --no-errors-on-unmatched --files-ignore-unknown=true",
        "compile-nobase": "tsc --project config/tsconfig-nobase.json"
    },
    "dependencies": {
        "url-join": "4.0.1",
        "form-data": "^4.0.0",
        "formdata-node": "^6.0.3",
        "node-fetch": "2.7.0",
        "qs": "^6.13.1",
        "readable-stream": "^4.5.2",
        "js-base64": "3.7.7",
        "async-mutex": "0.5.0",
        "eventemitter3": "5.0.1"
    },
    "devDependencies": {
        "@types/url-join": "4.0.1",
        "@types/qs": "^6.9.17",
        "@types/node-fetch": "^2.6.12",
        "@types/readable-stream": "^4.0.18",
        "webpack": "^5.97.1",
        "ts-loader": "^9.5.1",
        "jest": "^29.7.0",
        "@types/jest": "^29.5.14",
        "ts-jest": "^29.1.1",
        "jest-environment-jsdom": "^29.7.0",
        "@types/node": "^18.19.70",
        "prettier": "^3.4.2",
        "typescript": "~5.7.2",
        "@types/selenium-webdriver": "4.1.26",
        "@biomejs/biome": "1.9.4",
        "lefthook": "1.10.4"
    },
    "browser": {
        "fs": false,
        "os": false,
        "path": false
    },
    "author": {
        "name": "airtop.ai",
        "url": "https://www.airtop.ai/"
    },
    "description": "SDK for Airtop cloud browsers",
    "keywords": [
        "airtop",
        "ai",
        "cloud",
        "browser",
        "automation",
        "agent",
        "node",
        "typescript"
    ],
    "license": "MIT",
    "peerDependencies": {
        "playwright": ">=1.47.2",
        "puppeteer": ">=22.14.0",
        "selenium-webdriver": ">=4.25.0"
    }
}
